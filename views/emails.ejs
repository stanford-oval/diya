<%- include('header'); -%>

    <div class="row">
        <div class="col-sm">
            <h3>Emails</h3>
            <section class="box-wrap">
                <div>michael@michael.com</div>
                <div>sam@sam.com</div>
                <div>tom@tom.com</div>
            </section>
        </div>
        <div class="col-sm">
            <h3>GMail</h3>
            email:
            <br>
            <input>
            <br>
            <br>
            body:
            <br>
            <textarea rows="4" cols="50"></textarea>
            <br>
            <br>
            <button id='send'>Send</button>
            <br>
        </div>
    </div>

    <hr>

    <div class="row">
        <div class="col-sm">
            <h3>Script:</h3>
            <div class="col-sm">
                <div class="row">
                    <b>Say</b>: "start recording"
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Action</b>: [in the `email` field type] almond@stanford.edu 
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Action</b>: [in the `body` field type] Hello! 
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Action</b>: [double click `almond@stanford.edu`] 
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Say</b>: this is an email 
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Action</b>: [press tab] 
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Action</b>: [click send] 
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Say</b>: "stop recording" 
                </div>
            </div>

            <div class="col-sm">
                <div class="row">
                    <b>Say</b>: "name this program email"
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Say</b>: run email 
                </div>
            </div>
            <div class="col-sm">
                <div class="row">
                    <b>Say</b>: select emails on left side 
                </div>
            </div>
        </div>
        <div class="col-sm">
            <h3>Video:</h3>

            <iframe width="560" height="315" src="https://www.youtube.com/embed/5JUtTe00yGc" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
    </div>




<%- include('footer'); -%>