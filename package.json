{
  "name": "nightmare",
  "version": "0.7.1",
  "description": "A Chrome extension for recording browser interaction and generating scripts using natural language",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development DEBUG=nightmare:* webpack --watch",
    "build": "NODE_ENV=production webpack",
    "dist": "NODE_ENV=production webpack && node scripts/zip.js",
    "start-server": "node ./server/server.js",
    "test": "npm run unit-test && npm run e2e-test",
    "test-prod": "NODE_ENV=production npm run unit-test && npm run e2e-test",
    "unit-test": "jest __tests__/.*.spec.js --silent",
    "e2e-test": "jest __e2e-tests__ --runInBand --silent",
    "lint": "eslint --quiet -f codeframe src"
  },
  "pre-commit": [],
  "keywords": [
    "puppeteer",
    "chrome",
    "extension"
  ],
  "contributors": [
    "Tim Nolet",
    "Michael Fischer",
    "Giovanni Campagna"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/text-to-speech": "^2.3.0",
    "@medv/finder": "^1.1.2",
    "@simonwep/selection-js": "^1.6.0",
    "almond-dialog-agent": "~1.8.0",
    "annyang": "^2.6.1",
    "axios": "^0.19.2",
    "browserify": "^16.5.1",
    "bufferutil": "^4.0.1",
    "crx-hotreload": "^1.0.4",
    "date-fns": "^2.11.1",
    "easytimer.js": "^4.1.1",
    "ejs": "^3.0.2",
    "events": "^3.1.0",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1",
    "lowdb": "^1.0.0",
    "markdown": "^0.5.0",
    "micromodal": "^0.4.6",
    "morgan": "^1.10.0",
    "node-gettext": "^2.1.0",
    "optimal-select": "^4.0.1",
    "play-sound": "^1.1.3",
    "pug": "^2.0.4",
    "puppeteer-recorder": "^1.0.7",
    "regenerator-runtime": "^0.13.5",
    "sqlite3": "^4.1.1",
    "stemmer": "^1.0.5",
    "thingengine-core": "~1.8.0",
    "thingpedia": "^2.7.0",
    "thingtalk": "github:stanford-oval/thingtalk#master",
    "utf-8-validate": "^5.0.2",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.2.1",
    "vue-highlightjs": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^10.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-vue-preprocessor": "^1.7.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.13.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "puppeteer": "^1.20.0",
    "raw-loader": "^4.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.22.1",
    "superagent": "^3.8.3",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
